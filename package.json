{
  "name": "go-struct-auto-fill",
  "displayName": "Go Struct Auto Fill",
  "description": "Automatically fill Go struct fields with default values",
  "version": "1.3.5",
  "icon": "assets/images/logo.png",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:go"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "go-struct-auto-fill.fillStruct",
        "title": "Go Struct Auto Fill: Fill Struct Fields"
      }
    ],
    "keybindings": [
      {
        "command": "go-struct-auto-fill.fillStruct",
        "key": "alt+enter",
        "mac": "alt+enter",
        "when": "editorLangId == go"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Go Struct Auto Fill",
      "properties": {
        "go-struct-auto-fill.enableLog": {
          "type": "boolean",
          "default": true,
          "description": "在输出面板显示插件日志"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "keywords": [
    "go",
    "golang",
    "struct",
    "auto-fill",
    "code-generation",
    "productivity",
    "development"
  ],
  "author": "皇子",
  "license": "MIT",
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/node": "^16.11.7",
    "typescript": "^4.5.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leeprince/go-struct-auto-fill.git"
  },
  "publisher": "leeprince"
}